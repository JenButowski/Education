<Window x:Class="L11_T3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:L11_T3"
        mc:Ignorable="d"
        Title="L11" Height="400" Width="400">
    <Grid Name="grid">
        <Label Content="Введите значения матрицы A" HorizontalAlignment="Left" Margin="20,35,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="20,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="AMatrix"/>
        <Label Content="Введите значения матрицы B" HorizontalAlignment="Left" Margin="210,35,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="210,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="BMatrix"/>
        <Label Content="Вывод значений матрицы" HorizontalAlignment="Left" Margin="20,135,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.632,-0.465"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="20,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.409,6.191" Name="Result"/>

        <Button Content="Solve" HorizontalAlignment="Left" Margin="20,280,0,0" VerticalAlignment="Top" Width="100" Height="50"/>
    </Grid>
</Window>
